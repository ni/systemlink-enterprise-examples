                "endpoint": {
                    "url": "/nifile/v1/service-groups/Default/query-files-linq",
                    "method": "POST",
                    "body": {
                        "filter": "((properties.testPlanId == \"<id>\" && extension = \"json\") || <id == <fileIdsFromTemplate>>)",
                        "orderBy": "updated",
                        "orderByDescending": true,
                        "take": 1000
                    },
                    "detailBody": {
                        "filterTemplate": "<id == <ids>>",
                        "batchSize": 1000
                    }
                },
                "pageUrlTemplate": "/files/file/<id>/preview",
                "customFilterConfiguration": {
                    "key": "filter",
                    "expression": "&& (name.Contains(\"<input>\"))"
                },
                "responseMapping": {
                    "dataKey": "availableFiles",
                    "displayNameTemplate": "<properties.Name>",
                    "uniqueIdentifier": "id"
                }
            }
