            "i18n": [],
            "visible": true,
            "editable": true,
            "mandatory": false,
            "retainWhenHidden": false,
            "defaultValue": null,
            "requestConfiguration": {
                "endpoint": {
                    "url": "/niapm/v1/query-assets",
                    "method": "POST",
                    "body": {
                        "take": 1000,
                        "orderBy": "LAST_UPDATED_TIMESTAMP",
                        "descending": true,
                        "filter": "AssetType == \"DEVICE_UNDER_TEST\""
                    },
                    "detailBody": {
                        "filterTemplate": "<AssetIdentifier == <ids>>",
                        "batchSize": 1000
                    }
                },
                "pageUrlTemplate": "/assets/<id>",
                "customFilterConfiguration": {
                    "key": "filter",
                    "expression": "&& (AssetName.toLower().Contains(\"<input>\".toLower()))"
                },
                "responseMapping": {
                    "dataKey": "assets",
                    "displayNameTemplate": "<name>",
                    "uniqueIdentifier": "id"
                }
            }
        },
        {
            "workspace": "f58beba8-66de-4ba5-89f4-3388fd814f2e",
